	<queryString>
		<![CDATA[SELECT "NOMBRE",
       "UBICACION",
       "CAPACIDAD_AULA",
       "PIZARRA",
       "ORDENADORES"
FROM "INSTITUTO"."AULAS"
WHERE 
      ($P{nombre} IS NULL  
       OR LOWER("INSTITUTO"."AULAS"."NOMBRE") LIKE LOWER(CONCAT('%', $P{nombre}, '%')))
  AND ($P{ubicacion} IS NULL  
       OR LOWER("INSTITUTO"."AULAS"."UBICACION") LIKE LOWER(CONCAT('%', $P{ubicacion}, '%')))
  AND ($P{capacidad} IS NULL  
       OR CAST("INSTITUTO"."AULAS"."CAPACIDAD_AULA" AS VARCHAR(10)) LIKE CONCAT('%', $P{capacidad}, '%'))
  AND ($P{pizarra} IS NULL  
       OR "INSTITUTO"."AULAS"."PIZARRA" = $P{pizarra}) 
  AND ($P{ordenadores} IS NULL  
       OR "INSTITUTO"."AULAS"."ORDENADORES" = $P{ordenadores})]]>
	</queryString>
