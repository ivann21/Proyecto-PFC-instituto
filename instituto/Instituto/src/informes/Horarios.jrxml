	<queryString>
		<![CDATA[SELECT H."ID aula",
       H."dia de la semana",
       H."hora de inicio",
       H."hora de fin",
       P."NOMBRE" AS NOMBRE_PROFESOR,
       AU."NOMBRE" AS NOMBRE_AULA,
       ASIG."NOMBRE" AS NOMBRE_ASIGNATURA
FROM "INSTITUTO"."HORARIOS" H
INNER JOIN "INSTITUTO"."PROFESORES" P ON H."ID profesor" = P."ID profesor"
INNER JOIN "INSTITUTO"."AULAS" AU ON H."ID aula" = AU."ID aula"
LEFT JOIN "INSTITUTO"."ASIGNATURAS" ASIG ON H."ID asignatura" = ASIG."ID Asignatura"
WHERE ($P{diaSemana} IS NULL OR LOWER(H."dia de la semana") LIKE LOWER(CONCAT('%', $P{diaSemana}, '%')))
  AND ($P{horaInicio} IS NULL OR H."hora de inicio" = $P{horaInicio})
  AND ($P{horaFin} IS NULL OR H."hora de fin" = $P{horaFin})
  AND ($P{nombreProfesor} IS NULL OR LOWER(P."NOMBRE") LIKE LOWER(CONCAT('%', $P{nombreProfesor}, '%')))
  AND ($P{nombreAula} IS NULL OR LOWER(AU."NOMBRE") LIKE LOWER(CONCAT('%', $P{nombreAula}, '%')))
  AND ($P{nombreAsignatura} IS NULL OR LOWER(ASIG."NOMBRE") LIKE LOWER(CONCAT('%', $P{nombreAsignatura}, '%')))
]]>
	</queryString>
