	<queryString>
		<![CDATA[SELECT "INSTITUTO"."ALUMNOS"."NOMBRE" AS NOMBRE,
       "INSTITUTO"."ALUMNOS"."APELLIDO" AS APELLIDO,
       "INSTITUTO"."ALUMNOS"."CORREOELECTRONICO" AS CORREOELECTRONICO,
       "INSTITUTO"."CICLOS"."NOMBRE" AS CICLO,
       "INSTITUTO"."CICLOS"."ANIO" AS ANIO
FROM "INSTITUTO"."ALUMNOS"
JOIN "INSTITUTO"."CICLOS" ON "INSTITUTO"."ALUMNOS"."ID ciclos" = "INSTITUTO"."CICLOS"."ID Ciclos"
WHERE 
      ($P{nombreAlumno} IS NULL  
       OR LOWER("INSTITUTO"."ALUMNOS"."NOMBRE") LIKE LOWER(CONCAT('%', $P{nombreAlumno}, '%')))
  AND ($P{apellido} IS NULL  
       OR LOWER("INSTITUTO"."ALUMNOS"."APELLIDO") LIKE LOWER(CONCAT('%', $P{apellido}, '%')))
  AND ($P{correoElectronico} IS NULL  
       OR LOWER("INSTITUTO"."ALUMNOS"."CORREOELECTRONICO") LIKE LOWER(CONCAT('%', $P{correoElectronico}, '%')))
  AND ($P{nombreCiclo} IS NULL  
       OR LOWER("INSTITUTO"."CICLOS"."NOMBRE") LIKE LOWER(CONCAT('%', $P{nombreCiclo}, '%')))
  AND ($P{anioCiclo} IS NULL  
       OR CAST("INSTITUTO"."CICLOS"."ANIO" AS VARCHAR(10)) LIKE CONCAT('%', $P{anioCiclo}, '%'))
]]>
	</queryString>
